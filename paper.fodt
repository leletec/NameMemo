     <dc:date>2016-09-15T18:26:17</dc:date>
     <text:p text:style-name="P28"><text:span text:style-name="T20">Own headline for that?</text:span></text:p>
    </office:annotation></text:p>
   <text:p text:style-name="P4"/>
